<!--
    Written by: Haakon N. Unelsrød
    Error page. If an error occurs while interacting with the database, the user will be redirected here and the error will be displayed.
-->

@model System.Web.Mvc.HandleErrorInfo

@{
    ViewBag.Title = "Error";
}

<h1 class="text-danger">Error.</h1>
<h2 class="text-danger">Følgende feil oppstod mens forespørselen din ble behandlet.</h2>
<h2 class="text-danger">@Request.QueryString["e"]</h2>

